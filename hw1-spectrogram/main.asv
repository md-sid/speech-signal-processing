% This script shows different example parameters to use customSpectrogram
% 

clear; clc; close all;
[x, fs] = audioread("sx67.wav");

%% get the values and plot only spectrogram
params.Fs = fs;
params.nFFT = 1024;
% need to specify window type
% 0 for hanning, 1 for hamming, 2 for rectwin, 3 for kaiser, 4 for chebwin
params.window = [1, 200];
params.nOverlap = 50;

[s, f, t] = customSpectrogram(x, params);

%%
params.fsst = 0;
params.plotType = 1;
params.zoom = 1;
params.zoomTimeRange = []